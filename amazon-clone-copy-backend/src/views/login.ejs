<%- include('header', { title: 'Login' }) %>
<section>
  <h1>Login</h1>
  <form id="login-form">
    <label>Email</label>
    <input type="email" name="email" />
    <label>Password</label>
    <input type="password" name="password" />
    <button type="submit">Login</button>
  </form>
  <p>Don't have an account? <a href="/signup">Create one</a></p>
</section>

<%- include('footer') %>
