<%- include('header', { title: 'Sign up' }) %>
<section>
  <h1>Create account</h1>
  <form id="signup-form">
    <label>Name</label>
    <input type="text" name="name" />
    <label>Email</label>
    <input type="email" name="email" />
    <label>Password</label>
    <input type="password" name="password" />
    <button type="submit">Create account</button>
  </form>
</section>
<%- include('footer') %>
